<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">


    <SolidColorBrush x:Key="ButtonBackground" Color="#ddd" />


    <!--Заднеий план-->
    <SolidColorBrush x:Key="BackgroundSecondaryLightColor" Color="#92fd85" />
    <SolidColorBrush x:Key="BackgroundPrimaryLightColor" Color="#5ec956" />
    <SolidColorBrush x:Key="BackgroundMainColor" Color="#229727" />
    <SolidColorBrush x:Key="BackgroundPrimaryDarkColor" Color="#006800" />
    <SolidColorBrush x:Key="BackgroundSecondaryDarkColor" Color="#003c00" />

    <!--Предний план-->
    <SolidColorBrush x:Key="ForegroundLightColor" Color="#f1f1f1" />
    <SolidColorBrush x:Key="ForegroundDarkColor" Color="Black" />

    <!--Неактивные элементы-->
    <SolidColorBrush x:Key="ForegroundIsNotEnabled" Color="#838383" />
    <SolidColorBrush x:Key="BackgroundIsNotEnabled" Color="#f4f4f4" />

    <!--Кнопка Close-->
    <SolidColorBrush x:Key="BackgroudCloseButtonMouseOver" Color="#e81123" />
    <SolidColorBrush x:Key="ForegroundCloseButtonMouseOver" Color="#f1f1f1" />
    <SolidColorBrush x:Key="BackgroudCloseButtonPressed" Color="#e81123" />
    <SolidColorBrush x:Key="ForegroundCloseButtonPressed" Color="#FFF" />
    
    

</ResourceDictionary>